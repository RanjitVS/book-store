        if (data.error) {\r\n                setError(data.error)\r\n            }\r\n            else {\r\n                setProductsBySale(data)\r\n            }\r\n        })\r\n    }\r\n    const loadProductsByArrival = () => {\r\n        getProducts('createdAt').then(data => {\r\n            if (data.error) {\r\n                setError(data.error)\r\n            }\r\n            else {\r\n                setProductsByArrival(data)\r\n            }\r\n        })\r\n    }\r\n    useEffect(() => {\r\n        loadProductsByArrival()\r\n        loadProductsBySale()\r\n    }, [])\r\n\r\n    return (\r\n        <Layout  className='container-fluid '>\r\n            <div  className='container-fluid'>\r\n            < CarousalPage />\r\n            </div>\r\n\r\n            <h2 className='mb-4'> Whats New!</h2>\r\n            <hr />\r\n            <div className='row'>\r\n                {productsByArrival.map((product, i) => (\r\n                    <div className='col-4 mb-3' key={i}>\r\n                        <Card  product={product} />\r\n                    </div>\r\n                ))}\r\n            </div>\r\n\r\n            <h2 className='mb-4'>Best Sellers</h2>\r\n            <hr />\r\n            <div className='row'>\r\n                {productsBySale.map((product, i) => (\r\n                    <div key={i} className='col-4 mb-3'>\r\n                        <Card product={product} />\r\n                    </div>\r\n                ))}\r\n            </div>\r\n        </Layout>\r\n    )\r\n}\r\n\r\nexport default Home;","D:\\book-store\\frontend\\src\\user\\AdminDashboard.js",["156"],"import React from 'react';\r\nimport Layout from '../core/Layout';\r\nimport { isAuthenticated } from '../auth';//for gettig the info of the user loggedin\r\nimport { Link } from 'react-router-dom';\r\n\r\n\r\nconst AdminDashboard = () => {\r\n    const { user: { _id, name, email, role } } = isAuthenticated()\r\n    const adminLinks = () => {\r\n        return (\r\n          